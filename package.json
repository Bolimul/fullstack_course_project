{
  "name": "sce_fullstack",
  "version": "1.0.0",
  "description": "SCE FullStack Project",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node dist/src/Server.js",
    "dev": "copy .env_dev .env && nodemon ./src/Server.ts",
    "test": "copy .env_test .env && jest --detectOpenHandles --coverage --forceExit",
    "testUser": "copy .env_test .env && jest --detectOpenHandles --forceExit ./src/tests/student.test.ts",
    "testPost": "copy .env_test .env && jest --detectOpenHandles --forceExit ./src/tests/post.test.ts",
    "testAuth": "copy .env_test .env && jest --detectOpenHandles --forceExit --runInBand ./src/tests/auth.test.ts",
    "testFile": "copy .env_test .env && jest --detectOpenHandles --forceExit --runInBand  ./src/tests/file.test.ts",
    "lint": "eslint . --ext .ts"
  },
  "author": "Nikita Solonets",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "mz": "^2.7.0",
    "node": "^21.6.1",
    "openssl": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/mz": "^2.7.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
